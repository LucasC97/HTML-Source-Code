<script type="text/javascript">
function callDataUpdateValidation() { 
	var isFramed = false; 
	if(top.frames){ 
		if(top.frames.length){ 
			for(var i=0; i<top.frames.length; i++){ 
				isFramed=true; 
			} 
		} 
	} 
	if(!isFramed){ 
		top.location='/irj/servlet/prt/portal/prtroot/pcd!3aportal_content!2fPetronect_SRM!2fConteudoPrivado!2fRoles!2fAtualizaDados!2fAtualizaDados'; 
	}else{ 
		if(location.protocol=='http:'){ 
			var porta=''; 
			if(location.port){ 
				if(location.port!=null){ 
					if(location.port!='80'){ 
						var intPorta=parseInt(location.port,10); 
						var intPortaHTTPS=intPorta+1; 
						porta=':' + intPortaHTTPS ; 
					} 
				} 
			} 
		var action='https://' + location.hostname + porta + '/irj/servlet/prt/portal/prtroot/pcd!3aportal_content!2fPetronect_SRM!2fConteudoPrivado!2fRoles!2fAtualizaDados!2fAtualizaDados';
		window.open(action, "_self"); 
		}
		else if(location.protocol=='https:'){
			var porta=''; 
			if(location.port){ 
				if(location.port!=null){ 
					if(location.port!='80'){ 
						var intPorta=parseInt(location.port,10); 
						var intPortaHTTPS=intPorta+1; 
						porta=':' + intPortaHTTPS ; 
					} 
				} 
			}
			var action='https://' + location.hostname + porta + '/irj/servlet/prt/portal/prtroot/pcd!3aportal_content!2fPetronect_SRM!2fConteudoPrivado!2fRoles!2fAtualizaDados!2fAtualizaDados';
			window.open(action, "_self"); 
		} 
	}
}	

// INÍCIO ALTERACAO - PTS1200863 - Falha nos termos/CPF obrigatórios
function redirecionaLoginCheck() { 

	var isFramed = false; 
	if(top.frames){ 
		if(top.frames.length){ 
			for(var i=0; i<top.frames.length; i++){ 
				isFramed=true; 
			} 
		} 
	} 
	if(!isFramed){ 
		top.location='/sap/bc/yplpf/logincheck'; 

	}else{ 
		if(location.protocol=='http:'){ 
			var porta=''; 
			if(location.port){ 
				if(location.port!=null){ 
					if(location.port!='80'){ 
						var intPorta=parseInt(location.port,10); 
						var intPortaHTTPS=intPorta+1; 
						porta=':' + intPortaHTTPS ; 
					} 
				} 
			} 

		var action='https://' + location.hostname + porta + '/sap/bc/yplpf/logincheck';

		window.open(action, "_self"); 
		}
		else if(location.protocol=='https:'){
			var porta=''; 
			if(location.port){ 
				if(location.port!=null){ 
					if(location.port!='80'){ 
						var intPorta=parseInt(location.port,10); 
						var intPortaHTTPS=intPorta+1; 
						porta=':' + intPortaHTTPS ; 
					} 
				} 
			}

			var action='https://' + location.hostname + porta + '/sap/bc/yplpf/logincheck';

			window.open(action, "_self"); 
		} 
	}
}
// FIM ALTERACAO - PTS1200863 - Falha nos termos/CPF obrigatórios

