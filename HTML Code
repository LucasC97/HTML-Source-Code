/* Início alteração - Ricardo Ferreira - OS 815 - PORTAL-134.ES01 Criação de Velocímetro */
function openSpeedWindow(){
	var tab = document.getElementsByClassName("TabWorkCenter TabDown")[0].innerText.trim();
	tab = encodeURI(tab);
	var subTab = document.getElementsByClassName("Tab SubTabDown")[0].title.trim();
	subTab = encodeURI(subTab);
	window.open("https://www.petronect.com.br/sap(bD1wdCZjPTIwMA==)/bc/bsp/sap/ypvelo_monit/main.htm?var1=" + tab + "&var2=" + subTab, "", "width=560,height=760,resizable=yes"); 
}
/* Fim alteração - Ricardo Ferreira - OS 815 - PORTAL-134.ES01 Criação de Velocímetro */
